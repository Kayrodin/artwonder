<form method="post"  class="form-delete" action="{{ path('wond_art_delete', {'id': wond_art.id}) }}" onsubmit="return confirm('¿Estás seguro que quieres eliminar este wondart?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ wond_art.id) }}">
    <button class="btn borrar">Borrar</button>
</form>
